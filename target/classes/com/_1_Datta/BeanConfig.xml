<?xml version="1.0" encoding="UTF-8"?>


<!--XML Namespace and Schema Definitions: xmlns="http://www.springframework.org/schema/beans": 
	This declares the default namespace for the XML file, specifying that the 
	elements without a namespace prefix are part of the Spring Beans namespace. 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance": This declares the 
	XML Schema Instance namespace. xmlns:p="http://www.springframework.org/schema/p": 
	This declares a custom namespace prefix (p) for the "p" namespace. It's used 
	for concise property-based element configuration. xsi:schemaLocation: This 
	attribute specifies the location of the XML Schema Definitions (XSD) for 
	the declared namespaces. It's a space-separated list of namespace-URL pairs. -->

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd

">

	<!-- Injection of Primitive Datatype -->
	
	<!--1. property Injection By using tag -->
	<bean class="com._1_Datta.Player" name="sobj1">

		<property name="id">
			<value>45</value>
		</property>

		<property name="name">
			<value>Rohit</value>
		</property>

		<property name="Address">
			<value>Mumbai</value>
		</property>

	</bean>


	<!--2. property Injection By using Attribute -->
	<bean class="com._1_Datta.Player" name="sobj2">

		<property name="id" value="18" />

		<property name="name" value="Virat" />

		<property name="Address" value="Delhi" />

	</bean>


	<!--3. property Injection By using p Schema -->
	<bean class="com._1_Datta.Player" name="sobj3" p:id="10"
		p:name="Sachin Tendulkar" p:Address="Mumbai" />



</beans>

